<connections>
    <connection>
        <sender>useRangeCheckBox</sender>
        <signal>toggled(bool)</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>enableRange(bool)</slot>
    </connection>
    <connection>
        <sender>currentPageRadioButton</sender>
        <signal>clicked()</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>singleSelectable()</slot>
    </connection>
    <connection>
        <sender>evenPagesRadioButton</sender>
        <signal>clicked()</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>rangeSelectable()</slot>
    </connection>
    <connection>
        <sender>oddPagesRadioButton</sender>
        <signal>clicked()</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>rangeSelectable()</slot>
    </connection>
    <connection>
        <sender>allPagesRadioButton</sender>
        <signal>clicked()</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>rangeSelectable()</slot>
    </connection>
    <connection>
        <sender>fromPageSpinBox</sender>
        <signal>valueChanged(const QString&amp;)</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>checkRangeFrom()</slot>
    </connection>
    <connection>
        <sender>toPageSpinBox</sender>
        <signal>valueChanged(int)</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>checkRangeTo()</slot>
    </connection>
    <connection>
        <sender>okButton</sender>
        <signal>clicked()</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>accept()</slot>
    </connection>
    <connection>
        <sender>cancelButton</sender>
        <signal>clicked()</signal>
        <receiver>ApplyMasterPageDialog</receiver>
        <slot>reject()</slot>
    </connection>
</connections>

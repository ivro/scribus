                                            <string>List Non-Printing Entries</string>
                                        </property>
                                        <property name="toolTip" stdset="0">
                                            <string>Include frames that are set to not print as well</string>
                                        </property>
                                    </widget>
                                    <widget class="QComboBox" row="3" column="1">
                                        <property name="name">
                                            <cstring>itemParagraphStyleComboBox</cstring>
                                        </property>
                                        <property name="toolTip" stdset="0">
                                            <string>The paragraph style used for the entry lines</string>
                                        </property>
                                    </widget>
                                    <widget class="QLabel" row="3" column="0">
                                        <property name="name">
                                            <cstring>tocParagraphStyleLabel</cstring>
                                        </property>
                                        <property name="text">
                                            <string>Paragraph Style:</string>
                                        </property>
                                    </widget>
                                    <widget class="QLabel" row="1" column="0">
                                        <property name="name">
                                            <cstring>tocDestFrameLabel</cstring>
                                        </property>
                                        <property name="text">
                                            <string>Destination Frame:</string>
                                        </property>
                                    </widget>
                                </grid>
                            </widget>
